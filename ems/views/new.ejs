<!-- data entry -->
<%- include("header") %>
<%- include("navbar") %>
<div role="main" class="container">
  <div class="row">
    <h2>Data Entry</h2>
  </div>
  <form method="post" action="/process">
    <div class="form-group">
      <label for="firstNameInput">First Name</label>
      <input type="text" class=form-control id="firstNameInput" name="txtFirstName">
    </div>

    <div class="form-group">
      <label for="lastNameInput">Last Name</label>
      <input type="text" class=form-control id="lastNameInput" name="txtLastName">
    </div>
    <div class="form-group">
      <input name="_csrf" value="<%= csrfToken %>" type="hidden">
      <button type="submit" class="btn btn-primary">Submit</button>
    </div>
  </form>
  <div><a href="/list">Return to List</a></div>
  <%- include("footer") %>
</div>
