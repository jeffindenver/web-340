<!-- Tabular view of employee records -->
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <% employees.forEach( (item) => { %>
    <tr>
      <td><%= item.firstName %></td>
      <td><%= item.lastName %></td>
      <td><%= item.email %></td>
    </tr>
    <% }); %>
  </tbody>

</table>
