<!-- Tabular view of employee records -->
<%- include("header") %>
<%- include("navbar") %>
<div class="container">
  <div class="row">
    <h2>Employee Table</h2>
  </div>
  <div class="row">
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
        </tr>
      </thead>
      <tbody>
        <% employees.forEach( (item) => { %>
        <tr>
          <td><%= item.firstName %></td>
          <td><%= item.lastName %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
  <%- include("footer") %>
</div>

